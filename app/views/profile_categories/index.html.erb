    <div class="section">
		<!--[if !IE]>start title wrapper<![endif]-->
  		<div class="title_wrapper">
			  <span class="title_wrapper_top"></span>
				<div class="title_wrapper_inner">
				  <span class="title_wrapper_middle"></span>
					<div class="title_wrapper_content">
					  <h2>Category Focuses:</h2>
					</div>
				</div>
				<span class="title_wrapper_bottom"></span>
			</div>
			<!--[if !IE]>end title wrapper<![endif]-->

			<!--[if !IE]>start section content<![endif]-->
			<div class="section_content">
			  <span class="section_content_top"></span>
				<div class="section_content_inner">
          <div class="table_tabs_menu">
          <!--[if !IE]>start  tabs<![endif]-->
            <ul class="table_tabs">
							<li><a class="selected" href="#"><span><span>List</span></span></a></li>
						</ul>
						<!--[if !IE]>end  tabs<![endif]-->
						<a class="update" href="#" onclick="javascript:open_form('/profile_categories/new')">
              <span><span><em>Add New Record</em><strong></strong></span></span>
            </a>
					</div>

					<!--[if !IE]>start table_wrapper<![endif]-->
					<div class="table_wrapper">
						<div class="table_wrapper_inner">
							<table width="100%" cellspacing="0" cellpadding="0">
								<tbody><tr>
									<th style="width:125px;"><span>Title</span></th>
									<th class="photo"><span>Total Focuses</span></th>
									<th style="width: 75px;">Actions</th>
								</tr>
                
              <% @profile_categories.each do |category| %>
								<tr class="<%= cycle("first", "second") %> category_<%= category.id %> ">
									<td style="padding-left:6px"><%= category.title %></td>
									<td><%= link_to category.profiles.count, profiles_path(:pc_id=>category.id) %></td>
									<td>
										<div class="actions_menu">
											<ul>
												<li><%= link_to "Edit",edit_profile_category_path(category),:class=>"edit" %></li>
												<li>
                          <%= link_to "Delete",profile_category_path(category),:class=>"delete",:title=> protect_string(category.title) %>
                        </li>
											</ul>
										</div>
									</td>
								</tr>
              <% end %>
							</tbody></table>
						</div>
					</div>
					<!--[if !IE]>end table_wrapper<![endif]-->
				</div>

				<span class="section_content_bottom"></span>
			</div>
			<!--[if !IE]>end section content<![endif]-->
      <div id="form_area">
      </div>
		</div>

<script type="text/javascript">
  
  function onDelete(_title,_id){
    deleteMe(_title,
             "/profile_categories/"+_id,
             function(){
               hideLoading();
               $(".category_"+_id).fadeOut('slow',$(".category_"+_id).remove());
             });
  }

</script>